{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/register/*": ["app/(view)/(auth)/register/*"],
      "@/login/*": ["app/(view)/(auth)/login/*"],
      "@/atoms/*": ["app/components/atoms/*"],
      "@/clientComponents": ["app/components/clientComponents"],
      "@/components": ["app/components"],
      "@/fonts": ["app/shared/fonts"],
      "@/utils": ["app/shared/utils"],
      "@/assets/*": ["app/shared/assets/*"],
      "@/helpers/*": ["app/shared/helpers/*"],
      "@/apiRegister/*": ["app/api/register/*"],
      "@/config/*": ["app/config/*"],
      "@/api/*": ["app/api/*"],
      "@/app/*": ["app/*"]
    },
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "types/**.ts"
  ],
  "exclude": ["node_modules"]
}
